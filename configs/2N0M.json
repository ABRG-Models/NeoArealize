{
    // Global simulation parameters
    "steps" : 10000,                // Number of steps to simulate for
    "logevery": 5000,
    "plotevery": 50,
    "overwrite_logs": true,
    "logpath" : "./logs/2N0M",
    "hextohex_d" : 0.01,           // Hex to hex distance, determines num hexes
    "svgpath" : "./ellipse.svg",
    "dt" : 0.0001,
    "boundaryFalloffDist" : 0.01,
    "D" : 0.1,                // Diffusion constant for branch intensification
    "E" : 1,                  // Diffusion constant for branching into empty regions
    "F" : 0.0,                // Diffusion constant for branching away from other-axons
    "l" : 2,
    "k" : 3,
    // Array of parameters for N thalamocortical populations:
    "tc": [
        { "alpha" : 3, "beta" : 16, "xinit" :  0.00, "epsilon" : 1.0, "yinit" : -0.05, "sigmainit" : 0.1, "gaininit" : 0.7 },
        { "alpha" : 3, "beta" : 16, "xinit" :  0.15, "epsilon" : 1.0, "yinit" : -0.0, "sigmainit" : 0.1, "gaininit" : 0.5  }
    ], // end tc

    // Array of parameters for the guidance molecules
    "guidance": [
    ] // end guidance
}
